#include <stdio.h>

int maior_numero(int a, int b, int c) {
    int maior = a; 

    if (b > maior) {
        maior = b;
    }

    if (c > maior) {
        maior = c;
    }

    return maior;
}

int main() {
    int num1, num2, num3;

    printf("Digite tres numeros: ");
    scanf("%d %d %d", &num1, &num2, &num3);

    int resultado = maior_numero(num1, num2, num3);

    printf("O maior numero e: %d\n", resultado);

    return 0;
}

